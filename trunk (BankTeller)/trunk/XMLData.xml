              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:choice>
      </xs:complexType>
      <xs:unique name="Constraint1" msdata:PrimaryKey="true">
        <xs:selector xpath=".//Accounts" />
        <xs:field xpath="AccountNumber" />
      </xs:unique>
    </xs:element>
  </xs:schema>
